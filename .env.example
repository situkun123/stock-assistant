# Your OpenAI API Secret Key (starts with sk-...)
OPENAI_API_KEY=sk-xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# Koyeb Personal Access Token for deployment
KOYEB_TOKEN=koyeb_api_token_here

# --- Docker Registry Credentials ---
# Your Docker Hub or GitHub Container Registry username
DOCKER_USERNAME=your_username_here

# Your Docker Hub Personal Access Token or password
DOCKER_PASSWORD=your_password_or_token_here

# MotherDuck Cloud API Token for database access for logging
DUCK_DB_TOKEN=your_duck_db_token_here

# Comma-separated list of username:password pairs for authentication
AUTH_USERS=admin:yourpassword,user2:anotherpassword

# Enable or disable logging to duckdb default is false
ENABLE_LOGGING=false